{
  "name": "firstnpm-sass",
  "version": "1.0.0",
  "description": "Le projet permet d'améliorer vos compétences dans un environnement de travail réel.",
  "main": "index.js",
  "scripts": {
    "compile": "node-sass sass/main.scss css/style.css",
    "watch": "node-sass sass/main.scss css/style.css -w",
    "compressed": "node-sass sass/main.scss css/style.css -w --output-style=compressed",
    "prefix": "postcss --use autoprefixer sass/main.scss -o css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mednajjar/Project-C2N3.git"
  },
  "keywords": [],
  "author": "MOHAMMED NAJJAR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mednajjar/Project-C2N3/issues"
  },
  "homepage": "https://github.com/mednajjar/Project-C2N3#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "node-sass": "^7.0.0",
    "postcss-cli": "^7.1.0"
  }
}
